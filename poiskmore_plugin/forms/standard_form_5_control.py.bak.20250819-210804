from qgis.PyQt.QtWidgets import QWidget, QTableWidget, QVBoxLayout
class StandardForm5Control(QWidget):
    def __init__(self, parent=None):
        super().__init__(parent)
        layout = QVBoxLayout(self)
        self.table = QTableWidget()
        layout.addWidget(self.table)
    def initialize_component(self):
        # Инициализация таблицы/элементов (упрощено для QGIS)
        self.table.setRowCount(5)
        self.table.setColumnCount(3)
@dataclass
class IncidentData:
    id: str = ""
    operation_id: str = ""
    name: str = ""
    description: str = ""
    # Другие поля...
    def to_qgs_feature(self) -> QgsFeature:
        feature = QgsFeature()
        feature.setAttribute("name", self.name)
        return feature
class ExportMapAsRaster:
    def execute(self, output_path: str):
        settings = QgsMapSettings()
        job = QgsMapRendererJob(settings)
        job.start()
        job.waitForFinished()
        # Сохранение в файл (упрощено)
@dataclass
class SIT185Message:
    mmsi: Optional[str] = None
    imo: Optional[str] = None
    call_sign: str = ""
    vessel_name: str = ""
    # Другие поля...
    markers: List[QgsPointXY] = field(default_factory=list)
@dataclass
class SIT185Marker:
    position_type: int = 0  # SIT185PositionType
    probability: float = 0.0
    position: QgsGeometry = QgsGeometry.fromPointXY(QgsPointXY(0, 0))
